{
  "name": "tatoe",
  "version": "1.0.0",
  "scripts": {
    "predev": "docker-compose up -d db",
    "dev": "blitz dev",
    "build": "blitz build",
    "build:docker": "docker build -t mpppk/tatoe .",
    "codegen": "blitz prisma generate && blitz codegen",
    "start": "blitz start",
    "start:docker": "docker-compose up app",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "cloud-sql-proxy": "cloud_sql_proxy -instances=tatoe-app:asia-northeast1:common=tcp:5432",
    "bundle-analyzer": "ANALYZE=true blitz build"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@prisma/client": "2.30.3",
    "blitz": "0.39.0",
    "final-form": "4.20.4",
    "final-form-arrays": "3.0.2",
    "final-form-persist": "1.0.4",
    "firebase": "8.10.0",
    "firebase-admin": "9.12.0",
    "mui-rff": "3.0.16",
    "react": "alpha",
    "react-dom": "alpha",
    "react-error-boundary": "3.1.4",
    "react-final-form": "6.5.7",
    "react-final-form-arrays": "3.1.3",
    "react-firebaseui": "5.0.2",
    "zod": "3.11.6",
    "@next/bundle-analyzer": "11.1.2"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.34",
    "@welldone-software/why-did-you-render": "6.2.3",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "2.30.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.5",
    "prisma": "2.30.3",
    "typescript": "4.4.4"
  },
  "private": true
}
